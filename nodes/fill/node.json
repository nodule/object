{
  "name": "fill",
  "ns": "object",
  "description": "Fills an object with key/value pairs. Both key and in are array ports. Key must be fed the key names, and in will contain the values.",
  "phrases": {
    "active": "Filling object"
  },
  "ports": {
    "input": {
      "in": {
        "title": "Value(s)",
        "type": "array"
      },
      "keys": {
        "title": "Key(s)",
        "type": "array"
      },
      "object": {
        "title": "Object",
        "type": "object",
        "default": null
      }
    },
    "output": {
      "error": {
        "title": "Error",
        "type": "object"
      },
      "out": {
        "title": "out",
        "type": "object"
      }
    }
  }
}