{
  "name": "has",
  "ns": "object",
  "async": true,
  "description": "Does the object contain the given property? You can use dot notation to dig deeper into the object.",
  "phrases": {
    "active": "Determining whether the object has property {{input.path}}"
  },
  "require": {
    "dot-json": "0.x.x"
  },
  "ports": {
    "input": {
      "in": {
        "title": "Object",
        "type": "object",
        "async": true
      },
      "path": {
        "title": "Path",
        "type": "string"
      }
    },
    "output": {
      "yes": {
        "title": "Yes",
        "type": "object"
      },
      "no": {
        "title": "No",
        "type": "object"
      }
    }
  }
}
