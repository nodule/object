{
  "name": "oneOf",
  "ns": "object",
  "async": true,
  "description": "Selects a (nested) property outputing the first property available. data.in || data.that || .. etc. Sends an error of none of the properties matched.",
  "phrases": {
    "active": "Picking one of the properties"
  },
  "ports": {
    "input": {
      "in": {
        "title": "In",
        "type": "Object",
        "async": true
      },
      "path": {
        "title": "Path",
        "type": "array"
      }
    },
    "output": {
      "out": {
        "title": "Output",
        "type": "any"
      },
      "error": {
        "title": "Error",
        "type": "object"
      }
    }
  },
  "dependencies": {
    "npm": {
      "dot-object": "0.x.x"
    }
  }
}
